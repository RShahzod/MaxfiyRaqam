<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="uz.shox.netnomer"
    >

    <uses-permission android:name="android.permission.INTERNET" />


    <application

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NetNomer"
        >
        <activity
            android:name=".MobiuzSaytActivity"
            android:exported="false" />
        <activity
            android:name=".BeelineSaytActivity"
            android:exported="false"
            />
        <activity
            android:name=".UcellSaytActivity"
            android:exported="false" />
        <activity
            android:name=".UzmobileSaytActivity"
            android:exported="false" />
        <activity
            android:name=".MobiuzActivity"
            android:exported="false"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".UcellActivity"
            android:exported="false"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".BeelineActivity"
            android:exported="false"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".UzmobileActivity"
            android:exported="false"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".SplashScreen"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:screenOrientation="portrait"/>
    </application>

</manifest>